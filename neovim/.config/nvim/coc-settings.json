{
  "phpactor.enable": true,
  "phpactor.path":"~/.config/nvim/plugged/phpactor/bin/phpactor",
  "languageserver": {
      "golang": {
        "command": "gopls",
        "rootPatterns": ["go.mod"],
        "filetypes": ["go"]
      }
  },
  "yaml.schemas": {
      "kubernetes": "~/code/novax/novax-kumay-kubernetes/*.yaml"
  },
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-8",
      "path": "/usr/lib/jvm/java-8-openjdk-amd64/",
      "default": true
    }
  ]
}
